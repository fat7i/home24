{
    "name": "...",
    "require": {
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "App\\": "App/"
        },
        "files": [
            "resources/config.php",
            "helpers/functions.php",
            "App/routes/web.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "php -r \"copy('resources/config-example.php', 'resources/config.php');\""
        ]
    }
}
